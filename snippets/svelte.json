{
  "Svelte: Event Dispatcher": {
    "prefix": "svdispatch",
    "body": [
      "import { createEventDispatcher } from 'svelte';",
      "",
      "const dispatch = createEventDispatcher<{ $1: $2}>()",
      "$0"
    ],
    "description": "Svelte Event Dispatcher"
  },
  "Svelte: Head Tag": {
    "prefix": "svhead",
    "body": ["<svelte:head>", "\t$1", "</svelte:head>", "$0"],
    "description": "Svelte Head Tag"
  },
  "Svelte: For Each": {
    "prefix": "svforeach",
    "body": ["{#each $1 as $2 ($3)}", "\t$0", "{/each}", "$0"],
    "description": "Svelte For Each"
  },
  "Svelte: For Each else": {
    "prefix": "svforeachelse",
    "body": [
      "{#each $1 as $2 ($3)}",
      "\t$4",
      "{:else}",
      "\t$5",
      "{/each}",
      "$0"
    ],
    "description": "Svelte For Each else"
  },
  "Svelte: If": {
    "prefix": "svif",
    "body": ["{#if $1}", "\t$2", "{/if}", "$0"],
    "description": "Svelte If"
  },
  "Svelte: If Else": {
    "prefix": "svifelse",
    "body": ["{#if $1}", "\t$2", "{:else}", "\t$3", "{/if}", "$0"],
    "description": "Svelte If Else"
  },
  "Svelte: If Else If": {
    "prefix": "svifelseif",
    "body": ["{#if $1}", "\t$2", "{:else if $3}", "\t$4", "{/if}", "$0"],
    "description": "Svelte If Else If"
  },
  "Svelte: Await": {
    "prefix": "svawait",
    "body": ["{#await $1}", "\t$2", "{:then $3}", "\t$4", "{/await}", "$0"],
    "description": "Svelte Await"
  },
  "Svelte: Key": {
    "prefix": "svkey",
    "body": ["{#key $1}", "\t$2", "{/key}", "$0"],
    "description": "Svelte Key"
  }
}
