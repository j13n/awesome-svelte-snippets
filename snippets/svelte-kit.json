{
  "SvelteKit: Page": {
    "prefix": "skpage",
    "body": ["<script lang=\"ts\">", "   $1", "</script>", "", "$2"],
    "description": "SvelteKit Page"
  },
  "SvelteKit: Script": {
    "prefix": "skscript",
    "body": [
      "<script lang=\"ts\">",
      "   export let data;",
      "   $1",
      "</script>"
    ],
    "description": "SvelteKit Script"
  },
  "SvelteKit: Load": {
    "prefix": "skload",
    "body": [
      "export const load = async (${1:event}) => {",
      "   $2",
      "   return {",
      "     $3",
      "   };",
      "};"
    ],
    "description": "SvelteKit Load"
  },
  "SvelteKit: Actions": {
    "prefix": "skactions",
    "body": [
      "export const actions = {",
      "   ${1:default}: async (${2:event}) => {",
      "     $3",
      "   }",
      "}"
    ],
    "description": "SvelteKit Actions"
  },
  "SvelteKit: Hooks": {
    "prefix": "skhooks",
    "body": [
      "export const handle = async ({ request, resolve }) => {",
      "   $1",
      "   return resolve(request);",
      "};"
    ],
    "description": "SvelteKit Hooks"
  }
}
